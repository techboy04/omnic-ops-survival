settings
{
	main
	{
		Description: "Fight endless waves of omnics on this COD Zombies styled survival. View the github for more information: github.com/techboy04/omnic-ops-survival Code: 1JK1X"
		Mode Name: "Omnic Ops Survival"
	}

	lobby
	{
		Allow Players Who Are In Queue: Yes
		Map Rotation: Paused
		Match Voice Chat: Enabled
		Max Team 1 Players: 4
		Max Team 2 Players: 0
		Return To Lobby: Never
		Swap Teams After Match: No
	}

	modes
	{
		Skirmish
		{
			enabled maps
			{
				Eichenwalde
				Eichenwalde Halloween
				Havana
				Paris
			}
		}

		General
		{
			Allow Hero Switching: Off
			Game Mode Start: Immediately
			Hero Limit: Off
			Respawn As Random Hero: On
			Spawn Health Packs: Disabled
		}
	}

	heroes
	{
		Team 1
		{
			enabled heroes
			{
				D.Va
			}
		}

		Team 2
		{
			Orisa
			{
				Damage Received: 20%
				Health: 500%
				Projectile Speed: 95%
			}
		}

		General
		{
			D.Va
			{
				Spawn Without Mech: On
				Ultimate Ability Self-Destruct: Off
			}
		}
	}

	extensions
	{
		Beam Effects
		Play More Effects
		Spawn More Dummy Bots
	}
}

